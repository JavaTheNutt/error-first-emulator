    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-json": "^1.2.0",
    "mocha": "^4.0.1",
